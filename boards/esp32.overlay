// Enable ESP32 Wifi
&wifi {
	status = "okay";
};

// Enable W5500 Ethernet module
&spi2 {
	status = "okay";
	w5500: w5500@0 {
		compatible = "wiznet,w5500";
		label = "w5500";
		reg = <0x0>;
		spi-max-frequency = <10000000>;
    int-gpios = <&gpio0 2 GPIO_ACTIVE_LOW>;
    reset-gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
	};
};